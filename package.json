{
  "name": "githubtrending",
  "version": "1.0.0",
  "description": "This service is to check GitHub API and pull basic information about the most trending repositories using NodeJS(ExpressJS), Vue3 and MongoDB.",
  "private": true,
  "scripts": {
    "format": "eslint ./server --fix && eslint ./client --fix && prettier --write ./server && prettier --write ./client",
    "test": "node --experimental-vm-modules node_modules/jest/bin/jest.js"
  },
  "devDependencies": {
    "@babel/core": "^7.26.10",
    "@babel/preset-env": "^7.26.9",
    "@eslint/js": "^9.25.1",
    "babel-jest": "^29.7.0",
    "eslint": "^9.25.1",
    "eslint-plugin-jest": "^28.11.0",
    "eslint-plugin-prettier": "^5.2.6",
    "eslint-plugin-vue": "^10.0.0",
    "globals": "^16.0.0",
    "jest": "^29.7.0",
    "prettier": "^3.5.3",
    "supertest": "^7.1.0"
  },
  "imports": {
    "#server/*": "./*",
    "#client/*": "./*"
  },
  "browserslist": [
    "last 2 versions",
    "> 1%",
    "not ie < 10"
  ]
}
